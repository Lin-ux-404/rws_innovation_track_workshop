# Azure OpenAI configuration
AZURE_OPENAI_API_KEY=your_openai_api_key
AZURE_OPENAI_ENDPOINT=https://your-instance.openai.azure.com/
AZURE_OPENAI_MODEL_DEPLOYMENT_NAME=gpt-4o-mini
AZURE_OPENAI_EMBEDDING_DEPLOYMENT_NAME=embedding-ada-002

# APIM configuration
APIM_GATEWAY_URL=https://apim-uniqueid.azure-api.net
APIM_SUBSCRIPTION_KEY=your_subscription_key

# RAG Storage configuration
RAG_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=yourstorageaccount;EndpointSuffix=core.windows.net;AccountKey=your_storage_key
RAG_DOCUMENTS_CONTAINER_NAME=documents

# Azure AI Search configuration
SEARCH_SERVICE_ENDPOINT=https://yoursearchservice.search.windows.net
SEARCH_SERVICE_ADMIN_KEY=your_search_admin_key
SEARCH_INDEX_NAME=documents-index
SEARCH_SEMANTIC_CONFIG=documents-index-semantic-configuration
